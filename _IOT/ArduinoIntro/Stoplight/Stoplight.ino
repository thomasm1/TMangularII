/*  Simple Wire for blinking an LED connect to the digital pin 32*/#include "Stoplight.h"color state;void setup() {  #if DEBUG  Serial.begin(9600);  #endif  pinMode(RED_LED_OUT, OUTPUT);  pinMode(YELLOW_LED_OUT, OUTPUT);  pinMode(GREEN_LED_OUT, OUTPUT);  state = GREEN;}void loop() {    switch(state) {    case RED:      digitalWrite(RED_LED_OUT, HIGH);      digitalWrite(YELLOW_LED_OUT, LOW);      digitalWrite(GREEN_LED_OUT, LOW);      #if DEBUG      Serial.write("Red\n");      #endif      state = GREEN;      break;    case YELLOW:      digitalWrite(RED_LED_OUT, LOW);      digitalWrite(YELLOW_LED_OUT, HIGH);      digitalWrite(GREEN_LED_OUT, LOW);      #if DEBUG      Serial.write("Yellow\n");      #endif      state = RED;      break;    case GREEN:      digitalWrite(RED_LED_OUT, LOW);      digitalWrite(YELLOW_LED_OUT, LOW);      digitalWrite(GREEN_LED_OUT, HIGH);      #if DEBUG      Serial.write("Green\n");      #endif      state = YELLOW;      break;  }  delay(DELAY);}